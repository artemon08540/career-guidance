{
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "isVerified": {
      "type": "boolean",
      "default": false
    },
    "source": {
      "type": "enumeration",
      "enum": ["admin", "expert"],
      "default": "admin"
    },
    "vectorEntries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category-vector-entry.category-vector-entry",
      "mappedBy": "category"
    },
    "expertAnswers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::expert-answer.expert-answer",
      "mappedBy": "category"
    }
  }
}